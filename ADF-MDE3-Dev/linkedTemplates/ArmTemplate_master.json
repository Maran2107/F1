{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-MDE3-Dev"
		},
		"ls_adls_f1_servicePrincipalCredential": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalCredential' of 'ls_adls_f1'"
		},
		"ls_sqlserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_sqlserver'"
		},
		"ls_adls_f1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsmde3f1.dfs.core.windows.net/"
		},
		"ls_adls_f1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "5dcf8108-80d9-4413-9df6-80180edc9adb"
		},
		"ls_adls_f1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "921ea13c-3c41-48ae-973b-0c65c567713c"
		},
		"ls_sqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-server-mde3.database.windows.net"
		},
		"ls_sqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SampleDB"
		},
		"ls_sqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-MDE3-Dev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_f1_servicePrincipalCredential": {
						"value": "[parameters('ls_adls_f1_servicePrincipalCredential')]"
					},
					"ls_sqlserver_password": {
						"value": "[parameters('ls_sqlserver_password')]"
					},
					"ls_adls_f1_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_f1_properties_typeProperties_url')]"
					},
					"ls_adls_f1_properties_typeProperties_tenant": {
						"value": "[parameters('ls_adls_f1_properties_typeProperties_tenant')]"
					},
					"ls_adls_f1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ls_adls_f1_properties_typeProperties_servicePrincipalId')]"
					},
					"ls_sqlserver_properties_typeProperties_server": {
						"value": "[parameters('ls_sqlserver_properties_typeProperties_server')]"
					},
					"ls_sqlserver_properties_typeProperties_database": {
						"value": "[parameters('ls_sqlserver_properties_typeProperties_database')]"
					},
					"ls_sqlserver_properties_typeProperties_userName": {
						"value": "[parameters('ls_sqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}